{
    "name": "kieranfyi/admin",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "KieranFYI\\Admin\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kieran",
            "email": "kieran@andersons.bz"
        }
    ],
    "repositories": [
        {
            "url": "git@github.com:Kieran-FYI/misc.git",
            "type": "vcs"
        },
        {
            "url": "git@github.com:Kieran-FYI/logging.git",
            "type": "vcs"
        },
        {
            "url": "git@github.com:Kieran-FYI/roles.git",
            "type": "vcs"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "KieranFYI\\Admin\\Providers\\AdminPackageServiceProvider"
            ],
            "aliases": {
                "Admin": "KieranFYI\\Admin\\Facades\\Admin"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.19",
        "kieranfyi/roles-core": "dev-main"
    }
}
